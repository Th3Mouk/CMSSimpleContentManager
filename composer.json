{
    "name": "th3mouk/cms-content-manager-bundle",
    "type": "symfony-bundle",
    "description": "Simple content manager bundle for Symfony.",
    "keywords": [
        "cms", "content", "manager", "symfony", "bundle"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jérémy Marodon",
            "email": "marodon.jeremy@gmail.com"
        }
    ],
    "require": {
        "symfony/symfony": "^2.3",
        "sonata-project/admin-bundle": "^2.3",
        "twig/twig": "^1.12"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^0.5|^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Th3Mouk\\CMSSimpleContentManagerBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
